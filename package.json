{
  "name": "alamat-next",
  "version": "0.2.3",
  "description": "Alamat.com Next / React app",
  "main": "server.js",
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "dev1": "next dev",
    "dev": "node server.js",
    "build": "next build",
    "export": "next build && next export -o dist",
    "prestart": "npm run build",
    "start": "next start -p 8080",
    "start-production": "next build && ALAMAT_ENV=production NODE_ENV=production next start -p 8080",
    "start-staging": "next build && ALAMAT_ENV=staging NODE_ENV=production next start -p 8080",
    "clean-babel-cache": "rm -Rf ./node_modules/.cache/babel-loader/*",
    "lint": "npm run lint:js",
    "lint:withfix": "npm run lint:js -- --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern vendor/*",
    "lint:staged": "lint-staged",
    "postinstall": "# cp vendor/gsap/bonus-files-for-npm-users/* node_modules/gsap/",
    "deploy-dev": "ssh -i \"~/.ssh/development.pem\" ubuntu@deployment.wild.plus -t \"cd /var/www/dev && git pull && npm i --production && NODE_ENV=staging npm run build && pm2 restart dev\"",
    "deploy-staging": "ssh -i \"~/.ssh/development.pem\" ubuntu@deployment.wild.plus -t \"cd /var/www/staging && git pull && npm i --production && NODE_ENV=staging npm run build && pm2 restart staging\""
  },
  "lint-staged": {
    "**/*.js": "lint:eslint"
  },
  "author": "Michael Dihardja <michael@alamat.com>",
  "license": "ISC",
  "dependencies": {
    "@devexpress/dx-react-core": "^1.1.1",
    "@devexpress/dx-react-grid": "^1.1.1",
    "@devexpress/dx-react-grid-material-ui": "^1.1.1",
    "algoliasearch": "^3.26.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "debounce": "^1.1.0",
    "express": "^4.16.3",
    "fontfaceobserver": "^2.0.13",
    "google-map-react": "^0.34.0",
    "gsap": "^1.20.3",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "material-ui-time-picker": "^0.1.6",
    "next": "^5.0.0",
    "nextjs-dynamic-routes": "^2.1.7",
    "node-sass": "^4.8.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "radium": "^0.23.0",
    "react": "^16.3.2",
    "react-beautiful-dnd": "^6.0.2",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.9",
    "react-facebook-account-kit": "^0.1.1",
    "react-geolocation": "^1.0.3",
    "react-google-tag-manager": "^2.2.1",
    "react-instantsearch": "^5.0.3",
    "react-popper": "^0.8.2",
    "react-router-dom": "^4.2.2",
    "react-star-rating-component": "^1.4.1",
    "react-swipeable-views": "^0.12.13",
    "react-tap-event-plugin": "^3.0.2",
    "reactstrap": "5.0.0-beta",
    "rheostat": "^2.1.3",
    "styled-jsx": "^2.2.6",
    "styled-jsx-css-loader": "^0.2.0",
    "styled-jsx-plugin-sass": "^0.2.4",
    "webpack-glsl-loader": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-stage-0": "^6.24.1",
    "eslint-config-wild": "^0.2.3",
    "lint-staged": "^4.3.0",
    "husky": "^0.14.3",
    "babel-eslint": "8.0.2",
    "eslint": "^4.17.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "0.59.0",
    "flow-typed": "2.2.3",
    "jest": "21.2.1",
    "react-test-renderer": "16.0.0"
  },
  "eslintConfig": {
    "extends": "wild"
  }
}
